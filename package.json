{
  "name": "config-basic-webpack-js-ts-react",
  "version": "1.0.0",
  "description": "Configuracion Basica con Webpack para proyectos JS, TypeScript and React",
  "main": "src/index.js",
  "scripts": {
    "dev-container": "webpack --mode development --entry ./container/src/index.js --output-path ./container/dist",
    "build-container": "webpack --mode production --entry ./container/src/index.js --output-path ./container/dist",
    "dev": "webpack --config webpack.config.dev.js",
    "build": "webpack --mode production",
    "start": "webpack serve --config webpack.config.dev.js --open",
    "docs": "jsdoc -c jsdoc.json"
  },
  "keywords": [
    "webpack, Javascript"
  ],
  "author": "Manuel Melendez @manuelmelendez0",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "dotenv-webpack": "^7.0.2",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "image-webpack-loader": "^7.0.1",
    "jsdoc": "^3.6.7",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.15",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "sass-loader": "^11.0.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.18.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
