{
    "plugins": [],
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(node_modules|docs)"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "recurse": "true",
        "destination": "./docs/",
        "template": "node_modules/docdash",
        "_comentario": "Este es otro-> template: ./node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "theme": "dark"
        },
        "tutorials": "./docs-tutorials",
        "readme": "./README.md"
    }
}